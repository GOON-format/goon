{
  "name": "goon-monorepo",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build -w @goon-format/goon && npm run build -w @goon-format/cli",
    "test": "vitest run",
    "bench": "tsx benchmarks/run.ts",
    "benchmark:tokens": "tsx benchmarks/src/run-tokens.ts",
    "benchmark:features": "tsx benchmarks/src/feature-analysis.ts",
